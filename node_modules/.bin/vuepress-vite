#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/road/github/code/project/my-blog/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.18_@types+node@20.17.6_sass-embedded@1.8_yc7kllqwekwebukfa2dgkgvk4e/node_modules/vuepress/bin/node_modules:/mnt/d/road/github/code/project/my-blog/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.18_@types+node@20.17.6_sass-embedded@1.8_yc7kllqwekwebukfa2dgkgvk4e/node_modules/vuepress/node_modules:/mnt/d/road/github/code/project/my-blog/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.18_@types+node@20.17.6_sass-embedded@1.8_yc7kllqwekwebukfa2dgkgvk4e/node_modules:/mnt/d/road/github/code/project/my-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/road/github/code/project/my-blog/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.18_@types+node@20.17.6_sass-embedded@1.8_yc7kllqwekwebukfa2dgkgvk4e/node_modules/vuepress/bin/node_modules:/mnt/d/road/github/code/project/my-blog/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.18_@types+node@20.17.6_sass-embedded@1.8_yc7kllqwekwebukfa2dgkgvk4e/node_modules/vuepress/node_modules:/mnt/d/road/github/code/project/my-blog/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.18_@types+node@20.17.6_sass-embedded@1.8_yc7kllqwekwebukfa2dgkgvk4e/node_modules:/mnt/d/road/github/code/project/my-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vuepress/bin/vuepress-vite.js" "$@"
else
  exec node  "$basedir/../vuepress/bin/vuepress-vite.js" "$@"
fi
